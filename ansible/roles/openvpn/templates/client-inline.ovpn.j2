client
dev tun
proto {{ ovpn_proto_main }}
remote {{ le_domain }} {{ ovpn_port_main }}
resolv-retry infinite
nobind
persist-key
persist-tun

remote-cert-tls server
cipher AES-256-GCM
auth SHA256
verb 3

<ca>
{{ ca_content | trim }}
</ca>
<cert>
{{ cert_content | trim }}
</cert>
<key>
{{ key_content | trim }}
</key>
